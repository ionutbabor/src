{% comment %}<div class="grid">
  <div class="grid__item large--two-thirds push--large--one-sixth">
    {% section 'newsletter' %}
    <h2>{{ page.title }}</h2>
    <div class="rte">
      {{ page.content }}
    </div>
  </div>
</div>{% endcomment %}





{% for block in section.blocks %}
{% if block.type == 'content' %}

<div id="top">
  <div class="banner-page">
   <img class="img-responsive" src="{{ block.settings.banner-img | img_url: 'master' }}" alt="{{ block.settings.banner-img | remove_first: 'files/' }}">
   <div class="main">
    <h2>{{ page.title }}</h2>
    {{ page.content }}
  </div>
</div>
</div><!-- #top --> 



{% elsif block.type == 'our-services-1' %}

<div id="middle">
  <div class="wrapper">
    <div class="grid">
      <div class="grid__item large--one-half">
       {% if block.settings.text != blank %}
       <p>{{ block.settings.text }}</p>
       {% endif %}
     </div>
     <div class="grid__item large--one-half text-center">
      <img class="img-responsive" src="{{ block.settings.img-top | img_url: 'original' }}" alt="{{ block.settings.img-top | remove_first: 'files/' }}" >
    </div>
  </div>
</div>
</div><!-- #middle -->


{% elsif block.type == 'our-services-2' %}

<div id="middle2">
  <div class="wrapper">
    <div class="grid">
     <div class="grid__item large--one-half text-center">
      <img class="img-responsive" src="{{ block.settings.img-top | img_url: 'original' }}" alt="{{ block.settings.img-top | remove_first: 'files/' }}">
    </div>
    <div class="grid__item large--one-half">
     {% if block.settings.text != blank %}
     <p>{{ block.settings.text }}</p>
     {% endif %}
   </div>
 </div>
</div>
</div><!-- #middle -->




{% elsif block.type == 'testimonials' %}

<div class="testimonials">
  <div class="wrapper">
    <div class="test-container">
      {% if block.settings.text != blank %}
      <h2>{{ block.settings.text }}</h2>
      {% endif %}

      <div class="center slider">
        <div class="main">
          <div class="triangle-border">
           {% if block.settings.text1 != blank %}
           <p>{{ block.settings.text1 }}</p>
           {% endif %}
         </div>

         {% if block.settings.text1_author != blank %}
         <p class="test-name">{{ block.settings.text1_author }}</p>
         {% endif %}
       </div> 

       <div class="main">
        <div class="triangle-border">
         {% if block.settings.text2 != blank %}
         <p>{{ block.settings.text2 }}</p>
         {% endif %}
       </div>

       {% if block.settings.text2_author != blank %}
       <p class="test-name">{{ block.settings.text2_author }}</p>
       {% endif %}
     </div> 

     <div class="main">
      <div class="triangle-border">
       {% if block.settings.text3 != blank %}
       <p>{{ block.settings.text3 }}</p>
       {% endif %}
     </div>

     {% if block.settings.text3_author != blank %}
     <p class="test-name">{{ block.settings.text3_author }}</p>
     {% endif %}
   </div>   

   <div class="main">
    <div class="triangle-border">
     {% if block.settings.text4 != blank %}
     <p>{{ block.settings.text4 }}</p>
     {% endif %}
   </div>

   {% if block.settings.text4_author != blank %}
   <p class="test-name">{{ block.settings.text4_author }}</p>
   {% endif %}
 </div> 

</div>

<br />
<a href="/blogs/news">
  <div class="caption-button" style="width: 20%; text-align: center; float: right; position: absolute; bottom: 10%; right: 0%">
    <p><a href="/pages/their-stories">Read Their Experiences</a></p>
  </div>
</a>
</div>
</div>
</div><!-- testimonials -->




{% elsif block.type == 'shop-amputation' %}

<div id="shop">
  <div class="wrapper">
    <div class="grid">

     <div class="grid__item large--one-whole text-center">
      {% if block.settings.title != blank %}
      <h2>{{ block.settings.title }}</h2>
      {% endif %}

    </div>

    <div class="grid__item large--one-quarter">
      {% if block.settings.text1 != blank %}
      <p><a href="{{ block.settings.text1-url }}">{{ block.settings.text1 }}</a></p>
      {% endif %}
      <img class="img-responsive" src="{{ block.settings.img-top1 | img_url: 'original' }}" alt="{{ block.settings.img-top1 | remove_first: 'files/' }}">
    </div> 

    <div class="grid__item large--one-quarter">
      {% if block.settings.text2 != blank %}
      <p><a href="{{ block.settings.text2-url }}">{{ block.settings.text2 }}</a></p>
      {% endif %}
      <img class="img-responsive" src="{{ block.settings.img-top2 | img_url: 'original' }}" alt="{{ block.settings.img-top2 | remove_first: 'files/' }}">
    </div>  

    <div class="grid__item large--one-quarter">
      {% if block.settings.text3 != blank %}
      <p><a href="{{ block.settings.text3-url }}">{{ block.settings.text3 }}</a></p>
      {% endif %}
      <img class="img-responsive" src="{{ block.settings.img-top3 | img_url: 'original' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
    </div> 

    <div class="grid__item large--one-quarter">
      {% if block.settings.text4 != blank %}
      <p><a href="{{ block.settings.text4-url }}">{{ block.settings.text4 }}</a></p>
      {% endif %}
      <img class="img-responsive" src="{{ block.settings.img-top4 | img_url: 'original' }}" alt="{{ block.settings.img-top4 | remove_first: 'files/' }}">
    </div>

  </div>
</div>
</div><!-- #middle -->

{% elsif block.type == 'amputee-store-stocks' %}

<div id="amputee-store-stocks">
  <div class="wrapper">
    <div class="grid">
      <div class="grid__item large--one-half">

        <div class="grid-box">
          <div class="grid__item large--one-third">
           <ul id="left-box">
            <li>
              <img class="img-responsive" src="{{ block.settings.img-top1 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
            </li>
            <li>
              <img class="img-responsive" src="{{ block.settings.img-top2 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
            </li>
            <li>
              <img class="img-responsive" src="{{ block.settings.img-top3 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
            </li>
          </ul>
        </div>  
        <div class="grid__item large--one-third">
         <ul id="middle-box">
          <li>
            <img class="img-responsive" src="{{ block.settings.img-top4 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
          </li>
          <li>
            <img class="img-responsive" src="{{ block.settings.img-top5 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
          </li>
          <li>
            <img class="img-responsive" src="{{ block.settings.img-top6 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
          </li>
        </ul>
      </div>
      <div class="grid__item large--one-third">
       <ul id="right-box">
        <li>
          <img class="img-responsive" src="{{ block.settings.img-top7 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
        </li>
        <li>
          <img class="img-responsive" src="{{ block.settings.img-top8 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
        </li>
        <li>
          <img class="img-responsive" src="{{ block.settings.img-top9 | img_url: 'small' }}" alt="{{ block.settings.img-top3 | remove_first: 'files/' }}">
        </li>
      </ul>
    </div>

  </div><!-- grid -->


</div>
<div class="grid__item large--one-half no-padding">
  {% if block.settings.title != blank %}
  <h2>{{ block.settings.title }}</h2>
  {% endif %}


  <p>{{ block.settings.content }}</p>
</div>

</div>
</div>
</div><!-- #amputee-store-stocks -->

<div id="amputee-more-content">
 <div class="wrapper">
  <div class="grid">
    <div class="grid__item large--one-half push--large--one-quarter">
     <p>{{ block.settings.content2 }}</p>
   </div>
 </div>
</div>
</div><!-- amputee-more-content -->

{% endif %}
{% endfor %}


<!-- begin blog -->
<div class="wrapper main-content">
<div class="grid">

  <div class="header-container">
    <h2 class="page-title" style="margin-left: 30px; margin-top: 30px; margin-bottom: 30px;"><a href="/blogs/amputee-store-blog" style="font-family: 'bebas', sans-serif; color: #000; letter-spacing: 2px; font-size: 30px;">Amputee Store Blog</a></h2>
  </div>

  {% for article in blogs['amputee-store-blog'].articles limit: 3 %}

    <div class="grid__item large--one-third">

      <div class="rte blog-excerpt">
          {{ article.excerpt }}
          <h3><a href="{{ article.url }}">{{ article.title }}</a></h3>
          <p>{{ article.content | strip_html | truncatewords: 12 }}</p>
      </div>
      
      <p class="test-name" style="font-size: 20px;"><a href="{{ article.url }}">Read More</a></p>

    </div>

  {% endfor %}
</div>
</div>
<!-- end blog -->




