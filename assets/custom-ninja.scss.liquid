																																		#nav-toggle span, #nav-toggle span:before, #nav-toggle span:after {
																																			cursor: pointer;
																																			border-radius: 1px;
																																			height: 3px;
																																			width: 20px;
																																			background: {{ settings.pt_mobile_color  | default: '#222' }};
