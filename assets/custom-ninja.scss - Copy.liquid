.nav-bar{
	.large--one-quarter{
		text-align: center !important;
	}
	form.input-group.search-bar{
		float: left;
		background: #fff;
		color: #ccc;
		border: none;
		width: 100%;
		max-width: 100%;

		input{
			background-color: #fff;
			color: #a2a2a2 !important;
			height: 41px;
		}
		.input-group-btn{
			.icon-fallback-text{
				background: #7BCEC6;
				height: 41px;
				padding: 8px 20px;
				&:focus{
					outline:0;
				}
			}
		}
	}
	.move-right{
		.grid__item{
			padding-left: 0;
		}
		.social-menu{
			margin: 5px;
			li{
				padding-right: 0;
			}
			li:first-child{
				padding-right: 50px;
			}
			#content{
				font-family: 'Quicksand', sans-serif;
				font-size: 20px;
				color: #fff;
				/* font-weight: 700;*/
			}
			#coin-container{
				float:none;
				select{
					font-family: 'Quicksand', sans-serif;
					font-size: 20px;
					color: #fff;
					/*font-weight: 700;*/
					border: none;
					padding: 0;
				}
			}
		}
		
		.cart-right{
			margin: 0;
			float: right;
			li{
				display: inline-block;
				margin: 0;
			}
			#cart{
				a{
					float: none;
				}
			}
			#label{
				font-family: 'Quicksand', sans-serif;
				font-size: 20px;
				color: #fff;
				position: relative;
				top: -6px;
			}
		}
		/*drop down menu*/
		#primary_nav_wrap
		{
			margin-top:5px;
		}

		#primary_nav_wrap ul
		{
			list-style:none;
			position:relative;
			float:left;
			margin:0;
			padding:0;
			z-index: 1;
			text-transform: uppercase;
			color: #fff;
			font-family: 'Quicksand', sans-serif;
			font-size: 20px;
		}

		#primary_nav_wrap ul a
		{
			display:block;
			text-decoration:none;
			padding:5px 15px;
			text-transform: uppercase;
			color: #fff;
			font-family: 'Quicksand', sans-serif;
			font-size: 15px;
			.caret{
				display: inline-block;
				width: 0;
				height: 0;
				margin-left: 2px;
				vertical-align: middle;
				border-top: 4px dashed;
				border-top: 4px solid\9;
				border-right: 4px solid transparent;
				border-left: 4px solid transparent;
			}
		}

		#primary_nav_wrap ul li
		{
			position:relative;
			float:left;
			margin:0;
			padding:0
		}

		#primary_nav_wrap ul li.current-menu-item
		{
			background:#ddd
		}

		#primary_nav_wrap ul li:hover
		{
			background:#000;
		}

		#primary_nav_wrap ul ul
		{
			display:none;
			position:absolute;
			top:100%;
			left:0;
			background:#7BCEC6;
			padding:0;
			text-align: left;
		}

		#primary_nav_wrap ul ul li
		{
			float:none;
			width:200px
		}

		#primary_nav_wrap ul ul a
		{
			
			padding:10px 15px;
		}

		#primary_nav_wrap ul ul ul
		{
			top:0;
			left:100%
		}

		#primary_nav_wrap ul li:hover > ul
		{
			display:block
		}
		/* end drop down */
		#cart{
			position: relative;
			i{
				padding: 0 10px;
			}
			.icon-cart-count{
				position: absolute;
				top: -5px;
				right: 6px;
				color: orange;
				font-weight: bold;
			}
			
		}
	}
}

